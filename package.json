  "homepage": "https://corocoto.github.io/flappy-bird",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "local-build": "PUBLIC_URL=/ node scripts/build.js",
    "test": "node scripts/test.js --watchAll",
    "test:ci": "node scripts/test.js --watchAll=false",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eslint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "eslint-check": "eslint --ext .js,.jsx,.ts,.tsx .",
    "editorconfig-check": "editorconfig-checker",
    "e2e:visual": "start-test :3000/flappy-bird 'cypress open'",
    "e2e:ci": "start-test :3000/flappy-bird 'cypress run --spec \"cypress/integration/flappy-bird.spec.js\"'"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
